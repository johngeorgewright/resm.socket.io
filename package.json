{
  "name": "resm.socket.io",
  "version": "0.0.0",
  "description": "A RESM layer for Socket.io",
  "main": "build/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build dist",
    "compile": "lsc -cbo build src",
    "browserify": "mkdir dist && browserify -o dist/Client.js build/Client.js",
    "minify": "uglifyjs -mo dist/Client.min.js --source-map dist/Client.min.js.map build/Client.js",
    "prepublish": "npm run clean && npm run compile && npm run browserify && npm run minify",
    "setup-pre-commit": "rm -f .git/hooks/pre-commit && ln -s ../../git-hooks/pre-commit.sh .git/hooks/pre-commit"
  },
  "keywords": [
    "RESM",
    "Socket.io"
  ],
  "author": "John Wright <johngeorge.wright@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^6.1.0",
    "coffeeify": "^0.7.0",
    "uglify-js": "^2.4.15"
  },
  "dependencies": {
    "LiveScript": "^1.3.0",
    "prelude-ls": "^1.1.1"
  }
}
